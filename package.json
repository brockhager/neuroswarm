{
  "name": "neuroswarm",
  "version": "0.2.0",
  "description": "NeuroSwarm coordination and documentation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "package:bins": "node ./scripts/package-binaries.mjs",
    "lint:console": "node ./scripts/check-no-console.mjs",
    "lint:folders": "node ./scripts/check-repo-scoped-folders.mjs",
    "build": "tsc --project config",
    "test": "node --loader ts-node/esm tests/governance/test-quorum-validation.ts",
    "test:sources": "node tests/sources-root.test.mjs",
    "submit-proposals": "node scripts/governance/submit-bootstrap-proposals.ts",
    "docs": "echo 'Documentation is in docs/ folder'",
    "clone-all": "echo 'Use git clone for individual repos'",
    "test:sources-allie": "node scripts/integration/sources-allie.mjs --gateway http://localhost:8080 --vp http://localhost:4000 --ns http://localhost:3000 --timeout 60000"
  },
  "dependencies": {
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/web3.js": "^1.87.6",
    "express": "^4.19.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "uuid": "^9.0.1",
    "helia": "^4.0.0",
    "@helia/unixfs": "^3.0.0",
    "blockstore-fs": "^1.0.0",
    "datastore-fs": "^2.0.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "adm-zip": "^0.5.9",
    "open": "^9.0.0",
    "node-fetch": "^3.3.1",
    "pkg": "^5.8.0",
    "archiver": "^5.3.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^8.57.1",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brockhager/neuroswarm.git"
  },
  "keywords": [
    "neuroswarm",
    "ai",
    "blockchain"
  ],
  "author": "NeuroSwarm Team",
  "license": "MIT"
}